{
    "swagger": "2.0",
    "info": {
        "description": "API simple de ejemplo con Gin y Swagger",
        "title": "API Hello World",
        "contact": {},
        "version": "1.0"
    },
    "host": "localhost:8080",
    "basePath": "/api/v1",
    "paths": {
        "/hello": {
            "get": {
                "description": "Retorna un mensaje de saludo",
                "consumes": [
                    "application/json"
                ],
                "produces": [
                    "application/json"
                ],
                "tags": [
                    "hello"
                ],
                "summary": "Hello World",
                "responses": {
                    "200": {
                        "description": "OK",
                        "schema": {
                            "$ref": "#/definitions/main.HelloResponse"
                        }
                    }
                }
            }
        }
    },
    "definitions": {
        "main.HelloResponse": {
            "type": "object",
            "properties": {
                "message": {
                    "type": "string",
                    "example": "Hello World!"
                }
            }
        }
    }
}